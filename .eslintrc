{
    "env": {
        "browser": true,
        "amd": true
    },
    "globals": {
        "DocumentFragment": true,
        "console": true,
        "$": true
    },
    "rules": {
        "valid-jsdoc": 0,
        "curly": [1, "all"],
        "brace-style": [0, "stroustrup"],
        "consistent-this": 2,
        "no-constant-condition": 1,
        "no-underscore-dangle": 0,
        "no-console": 0,
        "no-use-before-define": [1, "nofunc"],
        "no-trailing-spaces": 0,
        "func-names": 0,
        "func-style": [2, "declaration"],
        "new-cap": 1,
        "new-parens": 2,
        "no-ternary": 0,
        "no-unused-vars": [0, {"vars": "local", "args": "none"}],
        "quotes": [0, "single"],
        "one-var": 0,
        "space-infix-ops": 0,
        "strict": 0,
        "eqeqeq": 0
    }
}
